    "normalize.css": "~5.0.0",
    "ping-ops-requests": "~1.0.4",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^6.0.3",
    "style-loader": "~0.15.0",
    "transform-loader": "^0.2.3",
    "url-loader": "~0.5.7",
    "uglifyjs-webpack-plugin": "~1.0.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "./node_modules/.bin/webpack-dev-server --host 127.0.0.1 --open --config config/webpack.config.js",
    "build": "npm run build:dist && cp -r /root/bittube-ipfs-chat/dist/* /var/www/html && chown -R www-data:www-data /var/www/html",
    "build:dist": "grunt build && webpack --config config/webpack.config.dist.js --hide-modules --progress --env production && uglifyjs dist/assets/app.js -o dist/assets/app.js",
    "build:debug": "grunt build && webpack --config config/webpack.config.js --display-modules --sort-modules-by size --progress --hot",
    "dist": "ipfs add -r dist/",
    "publish": "npm run dist | tail -1 | grep -o Qm[a-zA-Z0-9]* | xargs ping-ops-requests -d orbit.chat -t 'Deploy new version of orbit-web' -h",
    "start": "cd dist && http-server"
  }
}
