    "highlight.js": "^9.9.0",
    "ipfs": "^0.27.0",
    "ipfs-pubsub-room": "git+https://git@github.com/ipfs-shipyard/ipfs-pubsub-room.git",
    "lodash.differencewith": "^4.5.0",
    "lodash.flatten": "^4.4.0",
    "lodash.isarray": "^4.0.0",
    "lodash.mergewith": "^4.6.0",
    "lodash.pickby": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "lodash.without": "^4.4.0",
    "logplease": "^1.2.12",
    "object-assign": "^4.1.0",
    "orbit_": "orbitdb/orbit-core#fix/new-orbitdb",
    "p-each-series": "^1.0.0",
    "pleasejs": "^0.4.2",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-dropzone": "^3.9.2",
