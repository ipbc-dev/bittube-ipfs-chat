    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.27.3",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.10.0",
    "grunt": "~1.0.1",
    "grunt-contrib-clean": "~1.0.0",
